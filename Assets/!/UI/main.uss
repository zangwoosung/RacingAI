:root {
}

.default {
    height: 100%;
    transition-duration: 0.5s;
}

.down {
    translate: 0 100%;
    transition-duration: 0.5s;
    transition-timing-function: ease-in-out;
}

.p-default {
    flex-grow: 1;
    position: absolute;
    width: 100%;
    height: 100%;
    align-items: center;
    align-self: center;
    justify-content: center;
}

.dot {
    scale: 0 0 0;
    transition-duration: 0.3s;
}
